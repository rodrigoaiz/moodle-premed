# Variables de entorno para el SSO Backend
PORT=3001
SESSION_SECRET=supersecretkey_dev_mode # ¡CAMBIA ESTO POR UNA CADENA LARGA Y ALEATORIA EN PRODUCCIÓN!

# Credenciales del cliente OAuth2 configurado en Moodle
MOODLE_OAUTH_CLIENT_ID="your_moodle_oauth_client_id" # Reemplaza con el ID de tu cliente OAuth2 en Moodle
MOODLE_OAUTH_CLIENT_SECRET="your_moodle_oauth_client_secret" # Reemplaza con el Secreto de tu cliente OAuth2 en Moodle

# URL externa del portal Astro para redirecciones de SSO
# Usa localhost si estás desarrollando localmente, o tu IP fija de oficina si accedes remotamente
ASTRO_PORTAL_EXTERNAL_URL="http://localhost:4324"
